{
  "name": "cypress-swag-labs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run --headed",
    "cy:open": "npx cypress open --e2e --browser chrome",
    "daily-headed-chrome": "cypress run --headed --browser chrome --spec cypress/e2e/*.feature",
    "daily-headless-chrome": "cypress run --headless --browser chrome --spec cypress/e2e/*.feature",
    "ui-regression-allure": "npx cypress run --browser chrome --spec 'cypress/e2e/*.feature' --env allure=true",
    "ui-regression-headed-allure-electron": "npx cypress run --headed --browser electron --spec 'cypress/e2e/*.feature' --env allure=true",
    "ui-regression-headless-allure-electron": "npx cypress run --browser electron --headless --spec 'cypress/e2e/*.feature' --env allure=true",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
  },
  "repository": {
    "type": "git",
    "url": "kferko777"
  },
  "keywords": [],
  "author": "Kim Ferko",
  "license": "ISC",
  "type": "commonjs",
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/support/step_definitions/**/*.ts"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "cypress": "^15.8.1",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-fail-fast": "^7.1.1",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-multi-reporters": "^2.0.5",
    "cypress-real-events": "^1.15.0",
    "cypress-xpath": "^2.0.1",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3"
  }
}
